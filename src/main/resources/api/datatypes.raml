#%RAML 1.0 Library

types:
  Season:
    type: object
    description: Season Salesforce object
    properties:
      Id: 
        type: SalesforceUniqueId
      Name:
        type: string
      StartDate:
        type: date-only
      EndDate:
        type: date-only  
  Team:
    type: object
    description: Team Salesforce object
    properties:
      TeamName:
        type: string
      TeamId:
        type: SalesforceUniqueId
      AccountName:
        type: string
      AccountId:
        type: SalesforceUniqueId
  
  TeamCreateModel:
    type: object
    description: Team object for POST endpoint
    properties:
      TeamName:
        type: string
      ScoresProgramType:
        type: string
        required: false
      ProgramSite:
        type: string
        required: false
      CoachSoccer:
        type: SalesforceUniqueId
        required: false
      CoachWriting:
        type: SalesforceUniqueId
        required: false
      ScoresProgramCoordinator:
        type: SalesforceUniqueId
        required: false
      ScoresProgramManager:
        type: SalesforceUniqueId
        required: false
  
  TeamUpdateModel:
    type: object
    description: Team object for PATCH endpoint
    properties:
      TeamName:
        type: string
        required: false
      ScoresProgramType:
        type: string
        required: false
      ProgramSite:
        type: string
        required: false
      CoachSoccer:
        type: SalesforceUniqueId
        required: false
      CoachWriting:
        type: SalesforceUniqueId
        required: false
      ScoresProgramCoordinator:
        type: SalesforceUniqueId
        required: false
      ScoresProgramManager:
        type: SalesforceUniqueId
        required: false
      
  TeamSeasonBaseModel: 
    type: object
    description: Team Season Salesforce object
    properties:
      TeamSeasonName:
        type: string
      TeamSeasonId:
        type: SalesforceUniqueId
      TeamName:
        type: string
      SeasonName:
        type: string
      SchoolSite:
        type: string
      Partnership:
        type: string
      TotalNoOfPlayers:
        type: number
      TotalNoOfSessions:
        type: number
      SeasonStartDate:
        type: date-only
      SeasonEndDate:
        type: date-only
      CoachSoccer:
        type: SalesforceUniqueId
      CoachWriting:
        type: SalesforceUniqueId
      ProgramCoordinator:
        type: SalesforceUniqueId
      ScoresProgramManager:
        type: SalesforceUniqueId
      Region:
        type: string
        required: false
      SessionId:
        type: SalesforceUniqueId
        required: false
    examples: !include teamseason-base-examples.raml
      
  TeamSeason:
    type: TeamSeasonBaseModel
    properties:
      Season_Name:
        type: string
        required: false
      Team_Name:
        type: string
        required: false
      Sessions:
        type: array
        required: false
        items:
          type: Session
      Enrollments:
        type: array
        required: false
        items:
          type: Enrollment
    examples: !include teamseason-full-examples.raml
  Session:
    type: object
    description: Session of a team season
    properties:
      SessionId:
        type: SalesforceUniqueId
      SessionName:
        type: string
      SessionDate:
        type: date-only
      SessionTopic:
        type: string
        enum: [ "Soccer", "Writing", "Game_Day", "Soccer_and_Writing"]
      TeamSeasonId:
        type: SalesforceUniqueId
      AttendanceList:
        type: array
        required: false
        items:
          type: AttendanceUpdateModel
    examples: !include session-examples.raml

  TeamSeasonCreateModel:
    type: object
    description: TeamSeason object for POST endpoint
    properties:
      TeamSeasonName:
        type: string
      TeamId:
        type: SalesforceUniqueId
      SeasonId:
        type: SalesforceUniqueId
      SchoolSite:
        type: string
        required: false
      Partnership:
        type: string
        required: false
      TotalNoOfPlayers:
        type: number
        required: false
      TotalNoOfSessions:
        type: number
        required: false
      SeasonStartDate:
        type: date-only
        required: false
      SeasonEndDate:
        type: date-only
        required: false
      CoachSoccer:
        type: SalesforceUniqueId
        required: false
      CoachWriting:
        type: SalesforceUniqueId
        required: false

  TeamSeasonUpdateModel:
    type: object
    description: TeamSeason object for PATCH endpoint
    properties:
      TeamSeasonName:
        type: string
        required: false
      TeamId:
        type: SalesforceUniqueId
        required: false
      SeasonId:
        type: SalesforceUniqueId
        required: false
      SchoolSite:
        type: string
        required: false
      Partnership:
        type: string
        required: false
      TotalNoOfPlayers:
        type: number
        required: false
      TotalNoOfSessions:
        type: number
        required: false
      SeasonStartDate:
        type: date-only
        required: false
      SeasonEndDate:
        type: date-only
        required: false
      CoachSoccer:
        type: SalesforceUniqueId
        required: false
      CoachWriting:
        type: SalesforceUniqueId
        required: false
      ProgramCoordinator:
        type: SalesforceUniqueId
        required: false
      ProgramManager:
        type: SalesforceUniqueId
        required: false

  SessionCreateModel:
    type: object
    description: Session of a team season
    properties:
      SessionDate:
        type: date-only
      SessionTopic:
        type: string
        enum: [ "Soccer", "Writing", "Game_Day", "Soccer_and_Writing"]
      TeamSeasonId:
        type: SalesforceUniqueId
    examples: !include sessioncreatemodel-examples.raml

  SessionUpdateModel:
    type: object
    description: Session object for a PATCH endpoint
    properties:
      SessionName:
        type: string
        required: false
      SessionDate:
        type: date-only
        required: false
      SessionTopic:
        type: string
        enum: [ "Soccer", "Writing","Game_Day", "Soccer_and_Writing"]
        required: false
      TeamSeasonId:
        type: SalesforceUniqueId
        required: false
      Headcount:
        type: number
        required: false
      FemaleHeadcount:
        type: number
        required: false

  AttendanceCreateModel:
    type: object
    properties:
      SessionId:
        type: SalesforceUniqueId
      StudentId:
        type: SalesforceUniqueId
      Attended:
        type: boolean
  
  AttendanceUpdateModel:
    type: AttendanceBaseModel
    description: Attendance object for updating an attendance for a session
    properties:
      AttendanceId:
        type: SalesforceUniqueId
        required: false
      AttendanceName:
        type: string
        required: false
  AttendanceBaseModel:
    type: object
    description: Attendance of a session
    properties:
      SessionId:
        type: SalesforceUniqueId
        required: false
      SessionName:
        type: string
        required: false
      StudentId:
        type: SalesforceUniqueId
        required: false
      StudentName:
        type: string
        required: false
      RecordType:
        type: string
        required: false
      RecordedDate:
        type: date-only
        required: false
      Attended:
        type: boolean
    examples: !include attendance-examples.raml
  
  Enrollment:
    type: object
    description: Enrollement of a session
    properties:
      EnrollmentId:
        type: SalesforceUniqueId
      EnrollmentName:
        type: string
      SessionId:
        type: SalesforceUniqueId
      SessionName:
        type: string
      StudentId:
        type: SalesforceUniqueId
      FirstName:
        type: string
      LastName:
        type: string
      Birthdate:
        type: string
      Gender:
        type: string
      Ethnicity:
        type: string
      ZipCode:
        type: string
      StudentRecordComplete:
        type: boolean
      RecordType:
        type: string
      Date:
        type: date-only
      Attended:
        type: boolean
    examples: !include enrollment-examples.raml

  EnrollmentByContactID:
    type: object
    description: Enrollment data structure
    properties:
      Id: string
      TeamSeasonId: string
      TeamSeasonName: string
      StartDate: date-only
      EndDate: date-only
      SeasonName: string
      TeamID: string
      TeamName: string
      SchoolSiteId: string
      SchoolSiteName: string

  EnrollmentCreateModel:
    type: object
    description: Enrollement object for a POST endpoint
    properties:
      TeamSeasonId:
        type: SalesforceUniqueId
      StudentId:
        type: SalesforceUniqueId
      StartDate:
        type: date-only
        required: false
      EndDate: 
        type: date-only
        required: false

  EnrollmentUpdateModel:
    type: object
    description: Enrollement object for a PATCH endpoint
    properties:
      TeamSeasonId:
        type: SalesforceUniqueId
        required: false
      StudentId:
        type: SalesforceUniqueId
        required: false
      StartDate:
        type: date-only
        required: false
      EndDate: 
        type: date-only
        required: false

  SalesforceUniqueId:
    displayName: SalesforceUniqueId
    description: Unique Id of an object in Salesforce
    type: string
    maxLength: 18


